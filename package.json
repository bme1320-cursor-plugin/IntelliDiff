{
  "name": "intellidiff",
  "displayName": "IntelliDiff - Git Diff with AI Analysis",
  "version": "0.1.0",
  "publisher": "intellidiff",
  "description": "Advanced Git diff tool with AI-powered code change analysis",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhfeiyang/intellidiff.git"
  },
  "author": {
    "name": "IntelliDiff Team"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "activationEvents": [
    "onCommand:intellidiff.compareDiff",
    "onCommand:intellidiff.analyzeChanges"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "intellidiff.compareDiff",
        "title": "IntelliDiff: Compare Git Versions"
      },
      {
        "command": "intellidiff.analyzeChanges",
        "title": "IntelliDiff: Analyze Current Changes with AI"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "intellidiff.compareDiff",
          "group": "navigation"
        },
        {
          "command": "intellidiff.analyzeChanges",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "intellidiff-sidebar",
          "title": "IntelliDiff",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "intellidiff-sidebar": [
        {
          "id": "intellidiffExplorer",
          "name": "IntelliDiff Explorer"
        },
        {
          "id": "intellidiffAnalysis",
          "name": "AI Analysis"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "supports-color": "^10.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vsce": "^2.15.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "diff": "^5.1.0",
    "jimp": "^0.22.10",
    "simple-git": "^3.19.1"
  }
}
